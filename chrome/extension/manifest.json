{
  "name": "Songzify",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Songzify",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*.flickr.com/",
    "http://*.songza.com/",
    "http://*.spotify.com/",
    "http://*.youtube.com/"
  ]
}